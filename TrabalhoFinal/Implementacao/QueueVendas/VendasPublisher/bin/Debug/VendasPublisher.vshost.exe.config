<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IVendaServico" />
      </basicHttpBinding>
      <netMsmqBinding>
        <binding name="qLSC" exactlyOnce="false">
          <security mode="None" />
        </binding>
      </netMsmqBinding>
    </bindings>
    <client>
      <endpoint address="net.msmq://localhost/private/queueLSC" binding="netMsmqBinding"
        bindingConfiguration="qLSC" contract="ServicoVendas.IVendaServico"
        name="qLSC" />
      <!--<endpoint address="http://localhost:32737/VendaServico.svc" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IVendaServico" contract="ServicoVendas.IVendaServico"
        name="BasicHttpBinding_IVendaServico" />-->
    </client>
  </system.serviceModel>
</configuration>