<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <system.serviceModel>
    <services>
      <service name="QueueVendas.VendaServico" behaviorConfiguration="queueVendasBehavior">
        <endpoint name="qLSC" address="queueLSC" binding="netMsmqBinding" bindingConfiguration="queueVendasBinding" contract="QueueVendas.IVendaServico"/>
        <!-- <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>-->
        <host>
          <baseAddresses>
            <add baseAddress="net.msmq://localhost/private/"/>
            <!-- <add baseAddress="http://localhost:32578"/>-->
          </baseAddresses>
        </host>
      </service>
    </services>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_ISedeVendaServico" />
      </basicHttpBinding>
      <netMsmqBinding>
        <binding name="queueVendasBinding" exactlyOnce="false" >
          <security mode="None"></security>
        </binding>
      </netMsmqBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:55357/SedeVendaServico.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ISedeVendaServico"
        contract="SedeVendaServicoReference.ISedeVendaServico" name="BasicHttpBinding_ISedeVendaServico" />
    </client>
    <behaviors>
      <serviceBehaviors>
        <behavior name="queueVendasBehavior">
          <serviceMetadata httpGetEnabled="false"/>
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>