<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="Ex3.Servico">
        <!--behaviorConfiguration="sb"-->
        <!---->
        <host>
          <baseAddresses>
            <add baseAddress="net.msmq://localhost/private/filaWCF"/>
            <!--<add baseAddress="http://localhost:8001/Servico"/> -->
          </baseAddresses>
        </host>
        <endpoint address=""
                  binding = "netMsmqBinding"
                  contract="Ex3.IServicoMSMQ"
                  bindingConfiguration="MeuBinding"/>
        <!--
        <endpoint address = "mex"
                  binding = "mexHttpBinding"
                  contract = "IMetadataExchange"/>-->
        <!---->
      </service>
    </services>

    <!---->
    <behaviors>
      <serviceBehaviors>
        <behavior name="sb">
          <serviceMetadata httpGetEnabled="True"/>
        </behavior>
      </serviceBehaviors>

    </behaviors>
    <bindings>
      <netMsmqBinding>
        <binding name="MeuBinding"
                 receiveErrorHandling="Drop">
          <security mode="None">
          </security>
        </binding>
      </netMsmqBinding>
    </bindings>
  </system.serviceModel>
</configuration>
